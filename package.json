{
  "name": "turbocrawl",
  "version": "0.1.0",
  "description": "The simple and fast crawling framework.",
  "bin": {
    "tcrawl": "build/cli/tcrawl.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "chmod +x build/cli/tcrawl.js",
    "precrawl": "npm run build",
    "crawl": "node build/crawler.js",
    "doc": "typedoc --theme minimal",
    "prestart": "npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunnypurewal/TurboCrawl.git"
  },
  "keywords": [
    "crawl",
    "scrape",
    "webcrawl"
  ],
  "author": "Sunny Purewal (http://sunnypurewal.me)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sunnypurewal/TurboCrawl/issues"
  },
  "homepage": "https://github.com/sunnypurewal/TurboCrawl#readme",
  "dependencies": {
    "@microsoft/tsdoc": "^0.12.14",
    "@types/uuid": "^3.4.5",
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "getsitemap": "^0.7.8",
    "hittp": "^0.5.5",
    "ndjson": "^1.5.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/express": "^4.17.1",
    "@types/node": "^12.11.7",
    "@types/typescript": "^2.0.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  }
}
